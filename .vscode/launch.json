{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch WallTrek (Debug)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net9.0-windows10.0.19041.0/WallTrek2.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "requireExactSource": false
        },
        {
            "name": "Attach to WallTrek Process",
            "type": "coreclr",
            "request": "attach",
            "processName": "WallTrek2.exe"
        }
    ]
}